{
    "root": true,
    "parseroptions": {
      "ecmaversion": 6,
      "sourcetype": "module",
      "ecmafeatures": {
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "$": true
    },
    "rules": {
      "no-console": "off",
      "no-undef": "off"
    }
  }
  
  